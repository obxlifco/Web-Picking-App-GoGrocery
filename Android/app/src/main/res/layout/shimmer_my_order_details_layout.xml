<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    android:id="@+id/rl_shimmer"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:visibility="visible"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <LinearLayout
        android:id="@+id/layout_bottom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
       android:layout_alignParentTop="true"

        android:orientation="vertical">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_20sdp"
            android:layout_marginTop="12dp"
            android:layout_marginBottom="@dimen/_10sdp"
            android:orientation="vertical">
            <View
                android:layout_width="match_parent"
                android:background="@color/view_divider_clr"
                android:layout_marginEnd="@dimen/_14sdp"
                android:visibility="gone"
                android:layout_height="30dp">

            </View>
            <View
                android:layout_width="match_parent"
                android:background="@color/view_divider_clr"
android:layout_marginTop="14dp"
                android:layout_marginEnd="@dimen/_84sdp"
                android:layout_height="@dimen/_20sdp">

            </View>
            <View
                android:layout_width="match_parent"
                android:background="@color/view_divider_clr"
                android:layout_marginTop="14dp"
                android:layout_marginEnd="@dimen/_64sdp"
                android:layout_height="2dp">

            </View>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_20sdp"
            android:layout_marginEnd="@dimen/_20sdp"
            android:layout_marginTop="6dp"
            android:layout_marginBottom="@dimen/_10sdp"

            android:orientation="horizontal">
            <View
                android:layout_width="match_parent"
                android:background="@color/view_divider_clr"
                android:layout_weight="0.7"
                android:layout_marginEnd="@dimen/_4sdp"
                android:layout_height="20dp">

            </View>
            <View
                android:layout_width="match_parent"
                android:background="@color/view_divider_clr"
                android:layout_weight="0.3"
                android:layout_marginStart="@dimen/_10sdp"
                android:layout_height="20dp">

            </View>
            <View
                android:layout_width="match_parent"
                android:background="@color/view_divider_clr"
                android:layout_weight="1"
                android:visibility="gone"
                android:layout_marginStart="@dimen/_10sdp"
                android:layout_height="20dp">

            </View>
            <View
                android:layout_width="match_parent"
                android:background="@color/view_divider_clr"
                android:layout_weight="1"
                android:visibility="gone"
                android:layout_marginStart="@dimen/_10sdp"
                android:layout_height="20dp">

            </View>

        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:background="@color/view_divider_clr"
            android:layout_weight="1"
            android:layout_marginStart="@dimen/_20sdp"
            android:layout_marginBottom="4dp"
            android:layout_marginEnd="@dimen/_140sdp"
            android:layout_height="20dp">

        </View>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_20sdp"
            android:layout_marginEnd="@dimen/_20sdp"
            android:layout_marginTop="6dp"
            android:layout_marginBottom="@dimen/_10sdp"

            android:orientation="horizontal">
            <View
                android:layout_width="match_parent"
                android:background="@color/view_divider_clr"
                android:layout_weight="0.7"
                android:layout_marginEnd="@dimen/_4sdp"
                android:layout_height="20dp">

            </View>
            <View
                android:layout_width="match_parent"
                android:background="@color/view_divider_clr"
                android:layout_weight="0.3"
                android:layout_marginStart="@dimen/_10sdp"
                android:layout_height="20dp">

            </View>

            <View
                android:layout_width="match_parent"
                android:background="@color/view_divider_clr"
                android:layout_weight="1"
                android:visibility="gone"
                android:layout_marginStart="@dimen/_10sdp"
                android:layout_height="20dp">

            </View>

        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_20sdp"
            android:layout_marginEnd="@dimen/_20sdp"
            android:layout_marginTop="6dp"
            android:layout_marginBottom="@dimen/_10sdp"

            android:orientation="horizontal">
            <View
                android:layout_width="match_parent"
                android:background="@color/view_divider_clr"
                android:layout_weight="0.7"
                android:layout_marginEnd="@dimen/_4sdp"
                android:layout_height="20dp">

            </View>
            <View
                android:layout_width="match_parent"
                android:background="@color/view_divider_clr"
                android:layout_weight="0.3"
                android:layout_marginStart="@dimen/_10sdp"
                android:layout_height="20dp">

            </View>

            <View
                android:layout_width="match_parent"
                android:background="@color/view_divider_clr"
                android:layout_weight="1"
                android:visibility="gone"
                android:layout_marginStart="@dimen/_10sdp"
                android:layout_height="20dp">

            </View>

        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_20sdp"
            android:layout_marginTop="6dp"

            android:layout_marginBottom="@dimen/_10sdp"
            android:orientation="horizontal">
            <View
                android:layout_width="match_parent"
                android:background="@color/view_divider_clr"
                android:layout_weight="0.5"
                android:layout_marginEnd="@dimen/_14sdp"
                android:layout_height="30dp">

            </View>
            <View
                android:layout_width="match_parent"
                android:background="@color/view_divider_clr"
                android:layout_weight="0.5"
                android:layout_marginEnd="@dimen/_30sdp"
                android:layout_marginStart="@dimen/_3sdp"
                android:layout_height="30dp">

            </View>

        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:background="@color/view_divider_clr"
            android:layout_marginEnd="@dimen/_9sdp"
            android:layout_marginStart="@dimen/_9sdp"
            android:layout_height="4dp">

        </View>

    </LinearLayout>


    <LinearLayout
        android:id="@+id/buttom"
        android:layout_width="match_parent"
        android:layout_below="@+id/layout_bottom"

        android:layout_height="wrap_content"

        android:layout_marginBottom="@dimen/_3sdp"
        android:orientation="vertical">

        <LinearLayout

            android:layout_width="match_parent"
            android:layout_marginStart="@dimen/_20sdp"
            android:layout_marginBottom="@dimen/_10sdp"
            android:layout_height="wrap_content">
            <View
                android:layout_width="@dimen/_180sdp"
                android:background="@color/view_divider_clr"
                android:layout_height="30dp">

            </View>

        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_20sdp"
            android:layout_marginTop="6dp"
            android:orientation="horizontal">
            <View
                android:layout_width="match_parent"
                android:background="@color/view_divider_clr"
                android:layout_weight="0.5"
                android:layout_marginEnd="@dimen/_14sdp"
                android:layout_height="28dp">

            </View>
            <View
                android:layout_width="match_parent"
                android:background="@color/view_divider_clr"
                android:layout_weight="0.5"
                android:layout_marginStart="@dimen/_30sdp"
                android:layout_height="28dp">

            </View>

        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_20sdp"
            android:layout_marginTop="6dp"
            android:orientation="horizontal">
            <View
                android:layout_width="match_parent"
                android:background="@color/view_divider_clr"
                android:layout_weight="0.5"
                android:layout_marginEnd="@dimen/_14sdp"
                android:layout_height="28dp">

            </View>
            <View
                android:layout_width="match_parent"
                android:background="@color/view_divider_clr"
                android:layout_weight="0.5"
                android:layout_marginStart="@dimen/_30sdp"
                android:layout_height="28dp">

            </View>

        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_20sdp"
            android:layout_marginTop="6dp"
            android:orientation="horizontal">
            <View
                android:layout_width="match_parent"
                android:background="@color/view_divider_clr"
                android:layout_weight="0.5"
                android:layout_marginEnd="@dimen/_14sdp"
                android:layout_height="28dp">

            </View>
            <View
                android:layout_width="match_parent"
                android:background="@color/view_divider_clr"
                android:layout_weight="0.5"
                android:layout_marginStart="@dimen/_30sdp"
                android:layout_height="28dp">

            </View>

        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_20sdp"
            android:layout_marginTop="6dp"
            android:orientation="horizontal">
            <View
                android:layout_width="match_parent"
                android:background="@color/view_divider_clr"
                android:layout_weight="0.5"
                android:layout_marginEnd="@dimen/_14sdp"
                android:layout_height="28dp">

            </View>
            <View
                android:layout_width="match_parent"
                android:background="@color/view_divider_clr"
                android:layout_weight="0.5"
                android:layout_marginStart="@dimen/_30sdp"
                android:layout_height="28dp">

            </View>

        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_marginTop="@dimen/_20sdp"
            android:layout_marginBottom="@dimen/_10sdp"
            android:background="@color/view_divider_clr"
            android:layout_height="3dp">

        </View>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_20sdp"
            android:layout_marginTop="6dp"
            android:layout_marginBottom="@dimen/_10sdp"
            android:orientation="horizontal">
            <View
                android:layout_width="match_parent"
                android:background="@color/view_divider_clr"
                android:layout_weight="0.5"
                android:layout_marginEnd="@dimen/_14sdp"
                android:layout_height="40dp">

            </View>
            <View
                android:layout_width="match_parent"
                android:background="@color/view_divider_clr"
                android:layout_weight="0.5"
                android:layout_marginStart="@dimen/_30sdp"
                android:layout_height="40dp">

            </View>

        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/layout_top"
        android:layout_below="@+id/buttom"
        android:layout_width="match_parent"

        android:layout_height="wrap_content"
        android:orientation="vertical">
        <View
            android:layout_width="@dimen/_180sdp"
            android:layout_marginStart="@dimen/_20sdp"
            android:layout_marginTop="@dimen/_10sdp"
            android:background="@color/view_divider_clr"
            android:layout_height="34dp">

        </View>

        <LinearLayout

            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <View
                android:layout_width="@dimen/_100sdp"
                android:layout_height="@dimen/_80sdp"
                android:background="@color/view_divider_clr"
                android:layout_margin="@dimen/_20sdp">

            </View>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <View
                    android:background="@color/view_divider_clr"
                    android:layout_marginTop="@dimen/_20sdp"
                    android:layout_width="@dimen/_160sdp"
                    android:layout_height="@dimen/_28sdp">

                </View>
                <View
                    android:background="@color/view_divider_clr"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:layout_width="@dimen/_160sdp"
                    android:layout_height="@dimen/_24sdp">

                </View>

            </LinearLayout>

        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:visibility="gone"
            android:orientation="horizontal">
            <View
                android:layout_width="@dimen/_100sdp"
                android:layout_height="@dimen/_80sdp"
                android:background="@color/view_divider_clr"
                android:layout_marginTop="@dimen/_10sdp"
                android:layout_marginEnd="@dimen/_20sdp"
                android:layout_marginStart="@dimen/_20sdp">

            </View>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <View
                    android:background="@color/view_divider_clr"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:layout_width="@dimen/_160sdp"
                    android:layout_height="@dimen/_28sdp">

                </View>
                <View
                    android:background="@color/view_divider_clr"
                    android:layout_marginTop="@dimen/_20sdp"
                    android:layout_width="@dimen/_160sdp"
                    android:layout_height="@dimen/_38sdp">

                </View>

            </LinearLayout>

        </LinearLayout>

    </LinearLayout>




</RelativeLayout>